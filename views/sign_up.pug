extends layout 

block content 
  h1= title
  a( href='/' ) Go Back Home

  form( method='POST' action='' )
    label( for='userID' ) Username 
    input#userID(
      name='userID'
      placeholder='Enter Username'
      type='text'
    )
    label( for='password' ) Password 
    input#password(
      name='password'
      placeholder='Enter Password'
      type='password'
    )
    button( type='submit' ) Submit