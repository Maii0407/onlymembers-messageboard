extends layout

block content
  h1= title
  a( href='/' ) Go Back

  form( method='POST' action='' )
    label( for='userID' ) UserID:
    input#userID(
      type='text'
      placeholder='Enter UserID'
      name='userID'
    )
    label( for='password' ) Password:
    input#password(
      type='password'
      placeholder='Enter Password'
      name='password'
    )
    button( type='submit' ) Submit